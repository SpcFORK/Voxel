var string = "abcdef";

syscall io_out("String: ");
syscall io_out(string);
syscall io_out("\n");

syscall io_out("Size: ");
syscall io_out(string.size);
syscall io_out("\n");

syscall io_out("Length: ");
syscall io_out(string.length);
syscall io_out("\n");

syscall io_out("Character at index 2: ");
syscall io_out(string[2]);
syscall io_out("\n");

syscall io_out("Byte at index 2: ");
syscall io_out(string.byteAt(2));
syscall io_out("\n");

syscall io_out("Reversed: ");
syscall io_out(string.reverse());
syscall io_out("\n");

syscall io_out("Cut start: ");
syscall io_out(string.cutStart(4));
syscall io_out("\n");

syscall io_out("Cut end: ");
syscall io_out(string.cutEnd(4));
syscall io_out("\n");

syscall io_out("Substring: ");
syscall io_out(string.substring(2, 4));
syscall io_out("\n");

syscall io_out("Pad start: ");
syscall io_out(string.padStart(10, "123"));
syscall io_out("\n");

syscall io_out("Pad end: ");
syscall io_out(string.padEnd(10, "123"));
syscall io_out("\n");

syscall io_out("Repeat string: ");
syscall io_out(string.repeat(3));
syscall io_out("\n");

syscall io_out("String starts with ab: ");
syscall io_out(string.startsWith("ab"));
syscall io_out("\n");

syscall io_out("String starts with ef: ");
syscall io_out(string.startsWith("ef"));
syscall io_out("\n");

syscall io_out("String ends with ab: ");
syscall io_out(string.endsWith("ab"));
syscall io_out("\n");

syscall io_out("String ends with ef: ");
syscall io_out(string.endsWith("ef"));
syscall io_out("\n");

syscall io_out("String to buffer: ");
syscall io_out(string.toBuffer());
syscall io_out("\n");

var joinedString = "a bcd efg hi";

syscall io_out("Split string: ");
syscall io_out(joinedString.split(" "));
syscall io_out("\n");

var numericString = "1234";

syscall io_out("Numeric string to number + 1: ");
syscall io_out(numericString.toNumber() + 1);
syscall io_out("\n");

var multibyteString = "Tést © £10";

syscall io_out("Multibyte string size: ");
syscall io_out(multibyteString.size);
syscall io_out("\n");

syscall io_out("Multibyte string length: ");
syscall io_out(multibyteString.length);
syscall io_out("\n");

syscall io_out("Multibyte string substring: ");
syscall io_out(multibyteString.substring(0, 4));
syscall io_out("\n");

syscall io_out("Multibyte string substring: ");
syscall io_out(multibyteString.substring(7));
syscall io_out("\n");

syscall io_out("Multibyte string character at index 5: ");
syscall io_out(multibyteString[5]);
syscall io_out("\n");

syscall io_out("Multibyte string byte at index 4: ");
syscall io_out(multibyteString.byteAt(4));
syscall io_out("\n");